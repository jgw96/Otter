{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --port 3000",
    "dev": "npm run dev-server",
    "dev-task": "vite",
    "deploy": " npx @azure/static-web-apps-cli login --no-use-keychain && npx @azure/static-web-apps-cli deploy",
    "build": "tsc && vite build",
    "start": "npm run dev",
    "start-remote": "vite --host"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fluentui/web-components": "^2.5.12",
    "@khmyznikov/pwa-install": "^0.2.0",
    "@lit-labs/virtualizer": "^1.0.1",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@shoelace-style/shoelace": "^2.3.0",
    "@thepassle/app-tools": "^0.8.7",
    "@types/marked": "^4.0.8",
    "@vaadin/router": "^1.7.4",
    "blurhash-wasm": "^0.2.0",
    "browser-fs-access": "^0.33.0",
    "fast-blurhash": "^1.1.2",
    "hotkeys-js": "^3.10.1",
    "idb-keyval": "^6.2.0",
    "lit": "^2.6.1",
    "marked": "^4.2.12",
    "urlpattern-polyfill": "^6.0.2",
    "workbox-build": "^6.5.4",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.4"
  },
  "devDependencies": {
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-inject-preload": "^1.3.1",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-wasm": "^3.2.1"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
